{
  "name": "scioly-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "postinstall": "concurrently \"cd frontend && npm install\" \"cd backend && npm install\" \"cd bot && npm install\"",
    "all:lint": "eslint .",
    "format": "prettier --write .",
    "start": "cd backend && npm run backend:start",
    "all:build": "concurrently \"cd frontend && npm run frontend:build\" \"cd backend && npm run backend:build\" \"cd bot && npm run bot:build\"",
    "e2e": "start-server-and-test http://localhost:5000 'cypress open'",
    "e2e:headless": "start-server-and-test next:start http://localhost:3000 cypress:headless"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^7.1.0",
    "cypress": "^9.5.4",
    "dotenv": "^10.0.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regexp": "^1.7.0",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0"
  }
}
