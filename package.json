{
  "name": "scioly-bot",
  "workspaces": [
    "packages/*"
  ],
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write .",
    "build": "npm run build --workspaces",
    "start": "npm run start --workspace=packages/backend",
    "start:mock-app": "npm run start:mock-app --workspace=packages/backend",
    "test": "npm run test --workspaces --if-present",
    "e2e": "start-server-and-test http://localhost:5001 'cypress open'",
    "e2e:headless": "start-server-and-test http://localhost:5001 'cypress run",
    "e2e:mock-app": "cross-env CYPRESS_MOCK_APP=true start-server-and-test start:mock-app http://localhost:5001 'cypress open'",
    "e2e:mock-app:headless": "cross-env CYPRESS_MOCK_APP=true start-server-and-test start:mock-app http://localhost:5001 'cypress run'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cross-env": "^7.0.3",
    "cypress": "^9.5.4",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regexp": "^1.7.0",
    "jest": "^28.1.2",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2"
  }
}
