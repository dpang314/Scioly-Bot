{
	"compilerOptions": {
    "jsx": "react",
		"target": "es2017",
		"module": "commonjs",
		"outDir": "./bot-build",
    "rootDir": "./",
		"declaration": false,
		"strict": false,
		"sourceMap": true,
		"noImplicitAny": true,
		"alwaysStrict": false,
		"resolveJsonModule": true,
		"moduleResolution": "node",
		"typeRoots": [
      "./node_modules/@types"
    ]
	},
	"include": [
		//"./src/**/*"
		"src/sequelize.ts"
	],
	"exclude": [
		"./src/web/**",
		"./src/bot/deployCommands.ts",
	]
}